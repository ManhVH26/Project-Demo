CREATE TABLE User(
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL,
  email TEXT NOT NULL
);

selectAll:
SELECT * FROM User;

selectById:
SELECT * FROM User WHERE id = ?;

insertUser:
INSERT INTO User(name, email)
VALUES (?, ?);

updateUser:
UPDATE User SET name = ?, email = ? WHERE id = ?;

deleteUser:
DELETE FROM User WHERE id = ?;
